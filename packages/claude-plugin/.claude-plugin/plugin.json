{
  "name": "htmlgraph",
  "version": "0.26.0",
  "description": "HTML-based agent observability + workflow analytics with thin-shell hook architecture (Git-first JSONL + rebuildable SQLite index)",
  "author": {
    "name": "Shakes Dlamini"
  },
  "homepage": "https://github.com/Shakes-tzd/htmlgraph",
  "repository": "https://github.com/Shakes-tzd/htmlgraph",
  "license": "MIT",
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp",
        "--headless",
        "--isolated"
      ]
    }
  },
  "agents": {
    "gemini": {
      "executable": "agents/gemini-spawner.py",
      "model": "haiku",
      "description": "Google Gemini 2.0-Flash spawner - FREE exploratory research and batch analysis",
      "requires_cli": "gemini",
      "fallback": null,
      "capabilities": [
        "exploration",
        "analysis",
        "batch_processing"
      ],
      "context_window": "2M tokens",
      "cost": "FREE"
    },
    "codex": {
      "executable": "agents/codex-spawner.py",
      "model": "haiku",
      "description": "OpenAI Codex spawner - Code generation and completions with sandbox execution",
      "requires_cli": "codex",
      "fallback": null,
      "capabilities": [
        "code_generation",
        "implementation",
        "file_operations"
      ],
      "context_window": "128K tokens",
      "cost": "Paid (OpenAI)"
    },
    "copilot": {
      "executable": "agents/copilot-spawner.py",
      "model": "haiku",
      "description": "GitHub Copilot spawner - GitHub-integrated workflows and git operations",
      "requires_cli": "gh",
      "fallback": null,
      "capabilities": [
        "github_integration",
        "git_operations",
        "pr_handling"
      ],
      "context_window": "100K tokens",
      "cost": "Subscription (GitHub Copilot)"
    }
  }
}